<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="root_pane" minHeight="200" minWidth="300.0" stylesheets="stylesheetIcons.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <SplitPane fx:id="base_pane" dividerPositions="0.5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <ScrollPane hbarPolicy="NEVER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0">
                    <content>
                        <VBox fx:id="left_pane" spacing="10.0" />
                    </content>
                </ScrollPane>
                <AnchorPane fx:id="right_pane" />
            </items>
        </SplitPane>
      <VBox fx:id="right_box" alignment="TOP_CENTER" maxWidth="110.0" prefWidth="110.0">
         <children>
            <Button fx:id="button_truthTable" maxWidth="110.0" mnemonicParsing="false" prefWidth="110.0" text="Compute Truth Table" wrapText="true" />
            <Button fx:id="button_failure" maxWidth="110.0" mnemonicParsing="false" prefWidth="110.0" text="Compute Failure Table" wrapText="true" />
         </children></VBox>
    </children>
</fx:root>
